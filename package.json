{
  "name": "jthawme-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for running a Webpack workflow in Node express",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://johknee@bitbucket.org/johknee/react-boilerplate.git"
  },
  "keywords": [
    "express",
    "webpack",
    "node"
  ],
  "author": "Jonny Thaw",
  "homepage": "https://jthaw.me",
  "scripts": {
    "test": "",
    "start": "node server",
    "wordpress": "rimraf ../custom-theme && NODE_ENV=development webpack --config ./webpack/webpack.wordpress.config.js --watch",
    "wordpress-prod": "rimraf ../custom-theme && NODE_ENV=production webpack --config ./webpack/webpack.wordpress.config.js",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config ./webpack/webpack.production.config.js --progress --profile --colors",
    "eslint": "eslint .",
    "jscs": "jscs .",
    "component": "node ./config/new-component.js"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "classnames": "^2.2.5",
    "install": "^0.10.1",
    "normalize.css": "^7.0.0",
    "npm": "^5.5.1",
    "promise-polyfill": "^6.0.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "webfontloader": "^1.6.28",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jscs": "^2.0.5",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^4.1.2",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^1.1.5",
    "fs.extra": "^1.3.2",
    "html-webpack-plugin": "^2.30.1",
    "jscs": "^3.0.7",
    "jsdom": "^11.3.0",
    "json-loader": "^0.5.7",
    "mocha": "^4.0.1",
    "mocha-jsdom": "^1.1.0",
    "node-sass": "^4.6.1",
    "postcss-loader": "^2.0.8",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.6.2",
    "react-transform-hmr": "^1.0.4",
    "replace": "^0.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sass-material-colors": "0.0.5",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0",
    "stats-webpack-plugin": "^0.6.1",
    "style-loader": "^0.19.0",
