{
  "name": "jthawme-boilerplate",
  "version": "2.0.0",
  "description": "A boilerplate for running a Webpack workflow in Node express",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://johknee@bitbucket.org/johknee/react-boilerplate.git"
  },
  "keywords": [
    "express",
    "webpack",
    "node"
  ],
  "author": "Jonny Thaw",
  "homepage": "https://jthaw.me",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --hot --config ./webpack/webpack.config.js",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config ./webpack/webpack.production.config.js --progress --profile --colors",
    "component": "node ./helpers/new-component.js",
    "deploy": "./context/deploy.sh",
    "update:packages": "node ./helpers/wipe-dependencies.js && rimraf node_modules && npm update --save-dev && npm update --save"
  },
  "dependencies": {
    "@material-ui/core": "^3.8.3",
    "babel-cli": "^6.26.0",
    "classnames": "^2.2.6",
    "install": "^0.12.2",
    "normalize.css": "^8.0.1",
    "npm": "^6.5.0",
    "promise-polyfill": "^8.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-color": "^2.17.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.5.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "webfontloader": "^1.6.28",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jscs": "^3.0.0-beta1",
    "babel-loader": "^8.0.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-hot": "^1.0.5",
    "chai": "^4.2.0",
    "colors": "^1.3.3",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.2",
    "eslint": "^5.11.0",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^3.0.1",
    "fs.extra": "^1.3.2",
    "html-webpack-plugin": "^3.2.0",
    "jscs": "^3.0.7",
    "jsdom": "^13.1.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "mocha-jsdom": "^2.0.0",
    "node-sass": "^4.11.0",
